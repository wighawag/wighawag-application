<?xml version="1.0" encoding="utf-8" ?>
<project>
	<yoga-version>1</yoga-version>
	<compiler-version>2.10</compiler-version>
	
	<id>com.wighawag.application</id>
	<version>0.1.1-SNAPSHOT</version>

	<main>Main</main>
	
	<sources>
		<source path="src"/>
	</sources>
	
	<tests path="test" framework="munit"/>
	
	<targets hxml="compile">
		
		<target name="swf">
			<param line="-swf-version 11"/>
		</target>
		
		<target name="cpp"/>

		<target name="js"/>		

	</targets>
	
	
	<config-file output="project.nmml" template="nmml.template"/>

	<dependencies>
	       	<repository version="0.1.1-SNAPSHOT" id="com.wighawag.report.Report"/>
                <haxelib name="nme"/>
	</dependencies>
</project>
